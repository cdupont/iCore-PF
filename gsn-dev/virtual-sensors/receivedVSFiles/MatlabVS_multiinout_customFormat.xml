<virtual-sensor name="MatlabVS_multiinout_customFormat" priority="11">
	<processing-class>
		<class-name>gsn.vsensor.MatlabVS_multiinout_customFormat</class-name>
		<init-params>
			<param name="function-name">myGsnFunction</param>
			<param name="in-arguments">3</param>
			<param name="in-format">double:double:string</param>
			<param name="out-arguments">2</param>
			<param name="windows-size">10</param>
		</init-params>
		<output-structure>
			<field name="matlab_output_1" type="double"  />
			<field name="matlab_output_2" type="double"  />
		</output-structure>

	</processing-class>
	<description>this virtual sensor calls myMean function with 2 outputs</description>
	<life-cycle pool-size="2" />
	<addressing>
		<predicate key="geographical">ROOM 1</predicate>
	</addressing>
	<storage history-size="1" />
