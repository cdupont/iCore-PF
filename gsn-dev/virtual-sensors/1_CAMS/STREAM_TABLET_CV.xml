<virtual-sensor name="STREAM_TABLET_CV" priority="10">
	<processing-class>
		<class-name>gsn.vsensor.ImageRecognitionVS</class-name>
		<init-params>
			<param name="recognition">null</param>
		</init-params>
		<output-structure>
				<field name="image" type="binary:image/jpeg" />
		</output-structure>
	</processing-class>
	<description>This sensor captures frames from a opencvcam and interprets them as qr code or licence plate
	</description>
	<life-cycle pool-size="10" />
	<addressing>
		<predicate key="metadata">image metadata</predicate>
	</addressing>
	<streams>
		<stream name="RemoteCameraStream2">
			<source alias="RCS2" sampling-rate="1" storage-size="1">
				<address wrapper="opencvcam">
					<predicate key="webcam-name">STREAMCV</predicate>
					<predicate key="address">rtsp://192.168.9.228:8086</predicate>
					<predicate key="capture-rate">1000</predicate>
					<predicate key="max-posts">-1</predicate>
					<predicate key="live-view">true</predicate>
				</address>
				<query>select * from wrapper</query>
			</source>
			<query>select IMAGE from RCS2</query>
		</stream>
	</streams>
</virtual-sensor>


