<virtual-sensor name="slf_fft_vs" priority="10" >
    <processing-class>
       <class-name>ch.slf.FFTRealOneSided</class-name>
       <init-params>
			<param name="window-size">16</param>
			<param name="step-size">1</param>
		</init-params>
        <output-structure>
			<field name="DF" type="DOUBLE" />
			<field name="VALS" type="binary:text/plain" />
       </output-structure>
    </processing-class>
    <description>FFT VS</description>
    <life-cycle pool-size="10" />
    <addressing/>
    <storage history-size="10"/>
    <streams>
    <stream name="data">
      <source alias="source" storage-size="1" sampling-rate="1">
        <address wrapper="local">
        	<predicate key="name">ss_mem_vs</predicate>
        </address>
        <query> select * from wrapper </query>
      </source>
      <query> select * from source</query>
    </stream>
  </streams>
</virtual-sensor> 